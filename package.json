{
  "name": "lexi-boost-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
